<style>
	a.navbar-brand.navbar1 span {
		margin: 11px 0 0 6px;
		display: inline-block;
	}

	a.navbar-brand.navbar1 img {
		height: auto;
		width: 117px;
		float: left;
	}

	a.navbar-brand.navbar1 {
		padding: 3px;
	}

	.navbar-nav>.dropdown-user img {
		width: 35px;
		height: 35px;
		max-height: 140px;
	}
</style>
<!-- Main navbar -->
<div class="navbar navbar-inverse">
	<div class="navbar-header">
		<a class="navbar-brand navbar1" href="/dashboard"><img src="/images/logo3.png"alt=""> <span></span></a>
		<ul class="nav navbar-nav visible-xs-block">
			<li><a data-toggle="collapse" data-target="#navbar-mobile"><i class="icon-tree5"></i></a></li>
			<li><a class="sidebar-mobile-main-toggle"><i class="icon-paragraph-justify3"></i></a></li>
		</ul>
	</div>
	<div class="navbar-collapse collapse" id="navbar-mobile">
		<ul class="nav navbar-nav">
			<li><a class="sidebar-control sidebar-main-toggle hidden-xs"><i class="icon-paragraph-justify3"></i></a>
			</li>
		</ul>
		<ul class="nav navbar-nav navbar-right">
			<li class="dropdown dropdown-user">
				<a class="dropdown-toggle" data-toggle="dropdown">
					<img src="<%=/images/+session.user.profileimage%>" alt="">
					<span>
						<%= session.user.name%>
					</span>
					<i class="caret"></i>
				</a>
				<ul class="dropdown-menu dropdown-menu-right">
					<li class="<%= (title == 'profile_active'?'active':'') %>"><a href="/profile"><i
								class="icon-user-plus"></i> My profile</a></li>
					<li class="divider"></li>
					<li class="<%= (title == 'change_active'?'active':'') %>"><a href="/changepassword"><i
								class="icon-cog5"></i>Change Password</a></li>
					<li id="logout"><a href="/logout"><i class="icon-switch2"></i> Logout</a></li>
				</ul>
			</li>
		</ul>
	</div>
</div>